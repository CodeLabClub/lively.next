{
  "name": "lively.morphic",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/LivelyKernel/lively.morphic"
  },
  "dependencies": {
    "bowser": "^1.4.1",
    "lively.graphics": "https://github.com/LivelyKernel/lively.graphics",
    "lively.lang": "^0.7.0",
    "lively.serializer2": "https://github.com/LivelyKernel/lively.serializer2",
    "lively.bindings": "https://github.com/LivelyKernel/lively.bindings",
    "pepjs": "^0.4.1",
    "virtual-dom": "^2.1.1",
    "kld-intersections": "git://github.com/thelonious/kld-intersections#development",
    "gsap": "^1.19.0"
  },
  "devDependencies": {
    "jsdom": "^9.4.0",
    "mocha-es6": "^0.3.7"
  },
  "systemjs": {
    "main": "index.js",
    "map": {
      "mocha-es6": "./node_modules/mocha-es6/mocha-es6.js",
      "virtual-dom": "./node_modules/virtual-dom/dist/virtual-dom.js",
      "bowser": "./node_modules/bowser/bowser.js",
      "kld-intersections": "./node_modules/kld-intersections",
      "gsap": {
        "~node": "./node_modules/gsap/src/minified/TweenMax.min.js",
        "node": "@empty"
      },
      "gsap-css": {
        "~node": "./node_modules/gsap/src/minified/plugins/CSSPlugin.min.js",
        "node": "@empty"
      }
    },
    "meta": {
      "node_modules/mocha-es6/mocha-es6.js": {
        "format": "global",
        "exports": "mochaEs6"
      },
      "node_modules/virtual-dom/dist/virtual-dom.js": {
        "format": "amd"
      },
      "node_modules/bowser/bowser.js": {
        "format": "global",
        "exports": "bowser"
      },
      "node_modules/gsap/src/minified/*": {
        "format": "global"
      }
    },
    "packages": {
      "lively.morphic/node_modules/kld-intersections": {
        "main": "index.js",
        "meta": {
          "lib/Intersection.js": {
            "format": "cjs"
          },
          "lib/IntersectionParams.js": {
            "format": "cjs"
          },
          "node_modules/kld-affine/lib/Point2D.js": {
            "format": "global"
          }
        },
        "map": {
          "kld-affine": "./node_modules/kld-affine/index.js",
          "kld-polynomial": "./node_modules/kld-polynomial/index.js"
        }
      }
    }
  },
  "lively": {
    "main": "index.js",
    "packageMap": {
      "lively.serializer2": "./node_modules/lively.serializer2",
      "lively.graphics": "./node_modules/lively.graphics",
      "lively.lang": "./node_modules/lively.lang",
      "lively.bindings": "./node_modules/lively.bindings"
    }
  },
  "scripts": {
    "test": "mocha-es6 tests/{,text/}*-test.js"
  }
}