{
  "name": "lively-system-interface",
  "version": "0.1.0",
  "repository": "https://github.com/LivelyKernel/lively-system-interface",
  "dependencies": {
    "lively.modules": "*",
    "lively.resources": "*",
    "lively.lang": "*",
    "lively.ast": "*",
    "lively.vm": "*"
  },
  "devDependencies": {
    "mocha-es6": "^0.3.5",
    "systemjs": "^0.19.31"
  },
  "systemjs": {
    "map": {
      "mocha-es6": "./node_modules/mocha-es6/mocha-es6.js",
      "child_process": {
        "node": "@node/child_process",
        "~node": "@empty"
      },
      "fs": {
        "node": "@node/fs",
        "~node": "@empty"
      }
    }
  },
  "lively": {
    "main": "index.js",
    "packageMap": {
      "lively.modules": "./node_modules/lively.modules",
      "lively.resources": "./node_modules/lively.resources",
      "lively.lang": "./node_modules/lively.lang",
      "lively.ast": "./node_modules/lively.ast",
      "lively.vm": "./node_modules/lively.vm"
    }
  },
  "scripts": {
    "test": "mocha-es6 tests/*-test.js"
  }
}
