{
  "name": "lively.vm",
  "version": "0.6.1",
  "description": "Controlled JavaScript code execution and instrumentation.",
  "main": "index-node.js",
  "systemjs": {
    "main": "index.js",
    "map": {
      "path": {
        "node": "@node/path",
        "~node": "@empty"
      },
      "module": {
        "node": "@node/module",
        "~node": "@empty"
      },
      "fs": {
        "node": "@node/fs",
        "~node": "@empty"
      }
    }
  },
  "lively": {
    "packageMap": {
      "lively.lang": "./node_modules/lively.lang",
      "lively.ast": "./node_modules/lively.ast"
    }
  },
  "scripts": {
    "test": "mocha-es6 tests/*-test.js",
    "build": "node tools/build.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LivelyKernel/lively.vm.git"
  },
  "keywords": [
    "LivelyWeb",
    "JavaScript"
  ],
  "author": "Robert Krahn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LivelyKernel/lively.vm/issues"
  },
  "homepage": "https://github.com/LivelyKernel/lively.vm",
  "dependencies": {
    "lively.ast": "^0.7.0",
    "lively.lang": "^0.5.23"
  },
  "devDependencies": {
    "mocha-es6": "^0.2.9",
    "rollup": "^0.26.1",
    "rollup-plugin-node-builtins": "^1.0.4"
  }
}
