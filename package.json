{
  "name": "mocha-es6",
  "main": "index.js",
  "version": "0.3.6",
  "dependencies": {
    "glob": "^7.0.3",
    "lively.modules": "^0.5.0",
    "minimist": "^1.2.0",
    "systemjs": "^0.19.31"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-subset": "^1.2.1",
    "mocha": "^2.4.5",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "rollup": "^0.26.3",
    "rollup-plugin-babel": "^2.4.0"
  },
  "bin": {
    "mocha-es6": "bin/mocha-es6.js"
  },
  "scripts": {
    "build": "node tools/build.js"
  },
  "license": "MIT",
  "author": "Robert Krahn",
  "systemjs": {
    "main": "index.js",
    "meta": {
      "dist/mocha.js": {
        "format": "global",
        "exports": "mocha"
      },
      "dist/chai.js": {
        "format": "global",
        "exports": "chai"
      }
    },
    "map": {
      "chai": "./dist/chai.js",
      "mocha": "./dist/mocha.js",
      "fs": {
        "node": "@node/fs",
        "~node": "@empty"
      },
      "path": {
        "node": "@node/path",
        "~node": "@empty"
      }
    }
  },
  "lively": {
    "packageMap": {
      "lively.modules": "./node_modules/lively.modules"
    }
  }
}
