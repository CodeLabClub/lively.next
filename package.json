{
  "name": "lively.morphic",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/LivelyKernel/lively.morphic"
  },
  "dependencies": {
    "bowser": "^1.4.1",
    "lively.graphics": "https://github.com/LivelyKernel/lively.graphics",
    "lively.lang": "^1.0.0",
    "lively.serializer2": "https://github.com/LivelyKernel/lively.serializer2",
    "lively.bindings": "https://github.com/LivelyKernel/lively.bindings",
    "pepjs": "^0.4.1",
    "virtual-dom": "^2.1.1",
    "svg-intersections": "0.2.5",
    "gsap": "^1.19.0"
  }, 
  "devDependencies": {
    "jsdom": "^9.4.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-regenerator-runtime": "^6.5.0",
    "doc-comments": "^0.1.4",
    "lively.classes": "*",
    "lively.source-transform": "*",
    "mocha-es6": "^0.4",
    "rollup": "^0.36.1",
    "rollup-plugin-babel": "^2.6.1"
  },
  "systemjs": {
    "main": "index.js",
    "map": {
      "svg-intersections": "./dist/svg-intersections.js",
      "virtual-dom": "./node_modules/virtual-dom/dist/virtual-dom.js",
      "bowser": "./node_modules/bowser/bowser.js",
      "gsap": {
        "~node": "./node_modules/gsap/src/minified/TweenMax.min.js",
        "node": "@empty"
      },
      "gsap-css": {
        "~node": "./node_modules/gsap/src/minified/plugins/CSSPlugin.min.js",
        "node": "@empty"
      }
    },
    "meta": {
      "node_modules/virtual-dom/dist/virtual-dom.js": {
        "format": "amd"
      },
      "node_modules/bowser/bowser.js": {
        "format": "global",
        "exports": "bowser"
      },
      "node_modules/gsap/src/minified/*": {
        "format": "global"
      }
    }
  },
  "lively": {
    "main": "index.js",
    "packageMap": {
      "lively.serializer2": "./node_modules/lively.serializer2",
      "lively.graphics": "./node_modules/lively.graphics",
      "lively.lang": "./node_modules/lively.lang",
      "lively.bindings": "./node_modules/lively.bindings",
      "mocha-es6": "./node_modules/mocha-es6"
    }
  },
  "scripts": {
    "build": "node tools/build.js",
    "test": "mocha-es6 tests/{,text/}*-test.js"
  }
}
