{
  "name": "lively.morphic",
  "version": "0.1.0",
  "dependencies": {
    "virtual-dom": "^2.1.1",
    "lively.lang": "^0.6.2",
    "lively.graphics": "^0.1.0"
  },
  "devDependencies": {
    "mocha-es6": "^0.3.7"
  },
  "systemjs": {
    "main": "index.js",
    "map": {
      "mocha-es6": "./node_modules/mocha-es6/mocha-es6.js",
      "virtual-dom": "./node_modules/virtual-dom/dist/virtual-dom.js",
      "lively.lang": "./node_modules/lively.lang/dist/lively.lang.dev.js"
    },
    "meta": {
      "node_modules/virtual-dom/dist/virtual-dom.js": {
        "format": "amd"
      },
      "./node_modules/lively.lang/dist/lively.lang.dev.js": {
        "format": "global"
      }
    }
  },
  "lively": {
    "main": "index.js",
    "packageMap": {
      "lively.graphics": "./node_modules/lively.graphics"
    }
  },
  "scripts": {
    "test": "mocha-es6 tests/*-test.js"
  }
}