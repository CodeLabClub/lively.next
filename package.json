{
  "name": "lively.modules",
  "version": "0.5.1",
  "main": "dist/lively.modules.js",
  "repository": "https://github.com/LivelyKernel/lively.modules",
  "author": "Robert Krahn <robert.krahn@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "mocha-es6 tests/*-test.js",
    "build": "node tools/build.js"
  },
  "systemjs": {
