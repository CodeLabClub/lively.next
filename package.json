{
  "name": "lively.ast",
  "version": "0.6.0",
  "description": "Parsing JS code into ASTs and tools to query and transform these trees.",
  "main": "index.js",
  "systemjs": {
    "main": "es6-migration.js",
    "meta": {
      "node_modules/acorn": {
        "format": "global",
        "main": "dist/acorn.js"
      },
      "./dist/escodegen.browser.js": {
        "format": "global",
        "configured": true
      },
      "./dist/es6-runtime-config.json": {
        "format": "json"
      },
      "./node_modules/acorn/dist/acorn_loose.js": {
        "format": "global",
        "configured": true,
        "map": {
          "./acorn": "../acorn.js"
        }
      }
    },
    "map": {
      "acorn": "./node_modules/acorn/dist/acorn.js",
      "lively.lang": "node_modules/lively.lang",
      "escodegen": "./dist/escodegen.browser.js",
      "util": {
        "node": "@node/util",
        "~node": "@empty"
      },
      "child_process": {
        "node": "@node/child_process",
        "~node": "@empty"
      }
    },
    "packageConfigPaths": [
      "node_modules/*/package.json"
    ]
  },
  "scripts": {
    "test": "node_modules/mocha/bin/mocha tests/*-test.js && phantomjs node_modules/mocha-phantomjs-core/mocha-phantomjs-core.js tests/run-tests.html",
    "build": "node -e \"require('./build.js')();\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LivelyKernel/lively.ast.git"
  },
  "keywords": [
    "LivelyWeb",
    "parser",
    "parsing",
    "estree",
    "ast",
    "lively"
  ],
  "author": "Robert Krahn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LivelyKernel/lively.ast/issues"
  },
  "homepage": "https://github.com/LivelyKernel/lively.ast",
  "dependencies": {
    "acorn": "^2.7.0",
    "escodegen": "^1.8.0",
    "lively.lang": "^0.5.17"
  },
  "devDependencies": {
    "babel-core": "^5.8.35",
    "chai": "^3.3.0",
    "chai-subset": "^1.1.0",
    "estree-to-js": "^0.1.0",
    "mocha": "^2.3.3",
    "mocha-phantomjs-core": "^1.3.0",
    "systemjs": "^0.19.23",
    "systemjs-builder": "^0.15.10"
  }
}