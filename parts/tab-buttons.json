  "requiredVersion": "~0.1",
  "packages": {
    "local://lively-object-modules/": {
      "tab-buttons": {
        "package.json": "{\n  \"name\": \"tab-buttons\",\n  \"version\": \"0.1.0\",\n  \"lively\": {\n    \"isObjectPackage\": true\n  }\n}",
        "index.js": "import { Morph } from \"lively.morphic/morph.js\";\nimport { pt, Color, Rectangle } from \"lively.graphics/index.js\";\nimport { disconnect, connect, signal } from \"lively.bindings/index.js\";\nimport { arr } from \"lively.lang/index.js\";\nimport { show } from \"lively.morphic/index.js\";\n\nexport default class TabButtons extends Morph {\n\n  static get properties() {\n    return {\n      equalWidth: {defaultValue: true},\n      tabs: {defaultValue: []},\n      padding: {defaultValue: Rectangle.inset(8, 5)},\n      fontFamily: {defaultValue: \"sans-serif\"},\n      fontSize: {defaultValue: \"14\"},\n      activeTab: {},\n      theme: {defaultValue: \"light\"},\n      buttons: {\n        derived: true, after: [\"submorphs\"],\n        get() { return this.submorphs; },\n        set() {\n          this.buttons.forEach(ea => {\n            disconnect(ea, \"onMouseUp\", this, \"onTabClicked\");\n          });\n          this.submorphs = [];\n        }\n      }\n    }\n  }\n\n  reset() {\n    this.fill = null;\n    this.fontSize = 14\n    this.theme = \"light\";\n\n    // let props = this.constructor.properties, keys = Object.keys(props);\n    // keys.forEach(key => typeof this[key] === \"undefined\" && (this[key] = props[key].defaultValue))\n\n    this.buttons = [];\n    connect(this, 'activeTab', this, 'activateTab');\n    connect(this, 'tabs', this, 'update');\n    connect(this, 'theme', this, 'update');\n    connect(this, 'equalWidth', this, 'update');\n    connect(this, 'fontFamily', this, 'update');\n    connect(this, 'fontSize', this, 'update');\n    connect(this, 'padding', this, 'update');\n    this.tabs = [\"tab 1\", \"tab 2\", \"tab 3\"]\n  }\n\n  styleForTheme(theme = \"light\") {\n    let {padding, fontFamily, fontSize} = this,\n        base = {\n          padding, fontSize, fontFamily, borderRadius: 0,\n          nativeCursor: \"pointer\",\n          border: {width: 1, color: Color.gray}\n        };\n    if (theme === \"light\") {\n      return {\n        ...base,\n        fontColor: Color.rgbHex(\"666\"),\n        fill: Color.white\n      }\n    }\n    if (theme === \"dark\") {\n      return {\n        ...base,\n        fontColor: Color.white,\n        fill: Color.rgbHex(\"777\"),\n        border: {width: 0}\n      }\n    }\n    return base;\n  }\n\n  update() {\n    let {tabs} = this;\n    \n\n    if (!tabs.length) {\n      this.extent = pt(20, 20);\n      this.buttons = [];\n      return;\n    }\n\n    let {buttons, padding, fontFamily, fontSize} = this;\n\n    buttons.slice(tabs.length).forEach(ea => {\n      disconnect(ea, \"onMouseUp\", this, \"onTabClicked\");\n      ea.remove();\n    });\n\n    let x = 0, maxWidth = 0, maxHeight = 0;\n\n    tabs.forEach((tabName, i) => {\n      let btn = buttons[i],\n          props = {...this.styleForTheme(this.theme), value: tabName, left: x};\n      if (btn) {\n        Object.assign(buttons[i], props);\n      } else {\n        btn = this.addMorph({type: \"label\", ...props});\n        connect(btn, \"onMouseUp\", this, \"onTabClicked\",\n          {converter: function() { return this.sourceObj; }});\n      }\n      btn.fit();\n      x += Math.round(btn.width);\n      maxWidth = Math.round(Math.max(maxWidth, btn.width));\n      maxHeight = Math.round(Math.max(maxHeight, btn.height));\n    });\n\n    this.height = maxHeight;\n    if (this.equalWidth) {\n      this.width = this.submorphs.reduce((left, ea) => {\n        ea.left = left;\n        ea.width = maxWidth\n        return ea.right;\n      }, 0);\n    } else {\n      this.width = x;\n    }\n\n    \n    let first = this.buttons[0],\n        last = arr.last(this.buttons);\n\n    first.borderRadiusLeft = 10;\n    first.borderRadiusTop = 10;\n    first.borderRadiusBottom = 0;\n    first.borderRadiusRight = 0;\n    last.borderRadiusLeft = 0;\n    last.borderRadiusTop = 10;\n    last.borderRadiusBottom = 0;\n    last.borderRadiusRight = 10;\n\n    this.setProperty(\"activeTab\", null);\n  }\n\n  onTabClicked(tabButton) {\n    this.activateTab(tabButton);\n  }\n\n  activateTab(tab) {\n    let {buttons} = this,\n        tabButton = !tab ? null : tab.isMorph ?\n          tab : buttons.find(ea => ea.textString === tab),\n        baseBorder = this.styleForTheme(this.theme).border;\n    arr.without(buttons, tabButton).forEach(ea => {\n      ea.borderBottom = baseBorder;\n    });\n    if (tabButton) {\n      this.activeTab = tabButton.textString;\n      tabButton.borderBottom = {width: 4, color: Color.tangerine};\n    } else {\n      this.activeTab = null\n    }\n  }\n\n}\n\n"
      }
    }
  },
