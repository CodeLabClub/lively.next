      "Progress": {
        "package.json": "{\n  \"name\": \"Progress\",\n  \"version\": \"0.1.0\",\n  \"lively\": {\n    \"isObjectPackage\": true\n  }\n}",
        "index.js": "import { Morph } from \"lively.morphic\";\nimport { pt } from \"lively.graphics/index.js\";\nimport { connect, disconnect } from \"lively.bindings/index.js\";\n\nexport default class Progress extends Morph {\n\n  \n  static readme() {\n    `\n    Set property progressBar.progress to a value between 0 and 1 to modify the progress indicator.\n    progressBar.label to change its label.\n    \n    `\n  }\n\n  static get properties() {\n    return {\n      progress: {},\n      ui: {\n        derived: true, readOnly: true, after: [\"submorphs\"],\n        get() {\n          return {\n            filler: this.getSubmorphNamed(\"filler\"),\n            label: this.getSubmorphNamed(\"label\")\n          }\n        },\n      },\n      label: {\n        derived: true, after: [\"submorphs\"],\n        get() { return this.getSubmorphNamed(\"label\").value; },\n        set(val) {\n          this.getSubmorphNamed(\"label\").value = val;\n          this.getSubmorphNamed(\"label\").fit();\n          this.relayout();\n        }\n      }\n    }\n  }\n\n  reset() {\n    this.label = \"no label\";\n    this.clipMode = \"hidden\";\n    connect(this, 'progress', this, 'update');\n    connect(this, 'extent', this, 'update');\n  }\n\n  relayout() {\n    let {filler, label} = this.ui;\n    filler.position = pt(0,0);\n    filler.height = this.height;\n    label.center = this.innerBounds().center();\n  }\n\n  update() {\n    // this.getSubmorphNamed(\"label\").value = this.progress.toFixed(0.01)\n    let filler = this.ui.filler;\n    filler.width = Math.min(this.width, Math.max(0, this.width * this.progress));\n    this.relayout();\n  }\n\n}\n\n"
      }
    }
  },
