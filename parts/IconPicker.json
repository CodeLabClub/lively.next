        "index.js": "import { Morph } from \"lively.morphic\";\n\nexport default class IconPicker extends Morph {\n\n  beforePublish() {\n     this.clear()\n  }\n\n  clear() {\n     this.get('IconHolder').clear();\n  }\n\n  onLoad() {\n     this.get('IconHolder').onLoad();\n  }\n}\n\n"
      },
      "4C204C75-0D67-4468-A20A-1116B73F810F": {
        "package.json": "{\n  \"name\": \"4C204C75-0D67-4468-A20A-1116B73F810F\",\n  \"version\": \"0.1.0\"\n}",
        "index.js": "import { Text } from \"lively.morphic\";\nimport { Color } from \"lively.graphics/index.js\";\nimport { connect } from \"lively.bindings/index.js\";\n\nexport default class IconSearch extends Text {\n\n  static get properties() {\n    return {\n      haloShadow: {\n        defaultValue: {\n          blur: 6,\n          color: Color.rgb(52,152,219),\n          distance: 0,\n          rotation: 45\n        }\n      }\n    }\n  }\n\n  relayout() {\n     this.width = this.get('IconHolder').width\n  }\n\n  onMouseDown() {\n     this.animate({\n          dropShadow: this.haloShadow, \n          duration: 200,\n          textString: \"\",\n          fontColor: Color.black\n      })\n  }\n\n  onBlur() {\n     this.animate({\n         dropShadow: false, \n         fontColor: Color.gray,\n         textString: \"Search\",\n         duration: 200});\n  }\n}\n\n"
      },
      "256CA75B-9017-44D2-9927-28D566C35996": {
        "package.json": "{\n  \"name\": \"256CA75B-9017-44D2-9927-28D566C35996\",\n  \"version\": \"0.1.0\"\n}",
        "index.js": "import { Morph, morph } from \"lively.morphic\";\nimport { connect, signal } from \"lively.bindings/index.js\";\nimport { Color, pt } from \"lively.graphics/index.js\";\nimport { Icons } from \"lively.morphic/components/icons.js\";\n\nexport default class IconHolder extends Morph {\n  \n  static get properties() {\n     return {\n         icons: {after: ['submorphs'], get() {\n             return this._icons = this._icons || Object.keys(Icons).map(name => morph({\n               onHoverIn() { this.fill = new Color.rgb(133,193,233) },\n               onHoverOut() { this.fill = Color.transparent},\n               onMouseDown() { \n                 signal(this.get('IconPicker'), 'select', name)\n                 this.fill = this.fill.darker();\n               },\n               onMouseUp() { this.fill = new Color.rgb(133,193,233) },\n               readOnly: true,\n               type: \"text\", fontFamily: \"FontAwesome\", \n               textStyleClasses: [\"fa\"],\n               fill: Color.transparent,\n               tooltip: `${name}`, fontSize: 20, padding: 4, fixedWidth: true, extent: pt(40,40),\n               textString: `${Icons[name].code}`}\n             ));\n           }\n         }\n       }\n  }\n\n  clear() {\n     this._icons = [];\n     this.submorphs = [];\n  }\n\n  onLoad() {\n     this.submorphs = this.icons;\n  }\n\n  filterIcons() {\n     const searchTerm = this.get('IconSearch').textString;\n     if ([\"\", \"Search\"].includes(searchTerm))\n         return this.submorphs = this.icons;\n     this.submorphs = this.icons.filter(icon => \n         icon.tooltip.includes(searchTerm)\n     );\n  }\n}\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n"
      }
