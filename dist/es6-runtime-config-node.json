{
  "defaultJSExtensions": true,
  "transpiler": "babel",
  "babelOptions": {"stage": 2},
  "map": {
    "babel": "node_modules/babel-core/browser.js",
    "lively.lang": "node_modules/lively.lang",
    "lively.ast": "node_modules/lively.ast",
    "node_modules/lively.ast": {
      "acorn": "node_modules/lively.ast/node_modules/acorn",
      "escodegen": "lively.vm/node_modules/lively.ast/dist/escodegen.browser"
    },
    "callsite": "@empty",
    "path": "@node/path",
    "module": "@node/module",
    "fs": "@node/fs",
    "events": "@node/events",
    "util": "@node/util",
    "os": "@node/os",
    "child_process": "@node/child_process"
  },
  "packages": {
    "lively.vm": {"main": "index.js", "configured": true},
    "lively.lang": {"main": "index.js", "configured": true},
    "lively.ast": {"main": "index.js", "configured": true},
    "lively.ast/node_modules/acorn": {"main": "dist/acorn.js", "format": "cjs", "configured": true},
    "lively.ast/dist/escodegen.browser.js": {"format": "global", "configured": true}
  }
}