{
  "defaultJSExtensions": true,
  "transpiler": "babel",
  "babelOptions": {"stage": 2},
  "map": {
    "babel": "lively.vm/node_modules/babel-core/browser.js",
    "lively.lang": "lively.vm/node_modules/lively.lang",
    "lively.ast": "lively.vm/node_modules/lively.ast",
    "callsite": "@empty",
    "path": "@node/path",
    "module": "@node/module",
    "fs": "@node/fs",
    "events": "@node/events",
    "util": "@node/util",
    "os": "@node/os",
    "child_process": "@node/child_process"
  },
  "packages": {
    "lively.vm": {"main": "index.js", "configured": true},
    "lively.lang": {"main": "index.js", "configured": true, "format": "cjs"},
   "lively.ast": {
      "main": "index.js", "configured": true, "format": "cjs",
      "map": {
        "acorn": "./node_modules/acorn",
        "escodegen": "./dist/escodegen.browser.js"
      }
    },
    "acorn": {"main": "dist/acorn.js", "format": "cjs", "configured": true},
    "lively.ast/dist/escodegen.browser.js": {"format": "global", "configured": true}
  }
}